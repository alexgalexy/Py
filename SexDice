#Sex dice
import random
def mainF():
 x = random.choice([1,2,3,4])
 y = random.choice([1,2,3,4])
 #Define Functions
 def guyClean():
	
	#Clean
	
	#Action
	if x == 1:
		a = "Lick "
	elif   x == 2:
		a = "Tickle "
	elif  x == 3 :
		a = "Kiss "
	elif   x == 4:
		a = "Massage "
	#body part
	
	if y == 1:
		b = "Neck"
	elif  y == 2:
		b = "Lips"
	elif  y  == 3 :
		b = "Thighs"
	elif y == 4:
		b = "Leg"
	print a + b		
 def girlClean():
