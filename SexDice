		b = "Neck"
	elif y == 2:
		b = "Lips"
	elif y == 3:
		b = "Thighs"
	elif y == 4:
		b = "Cheek"
	print a + b		
	
	#Dirty
	
	
 def guyDirty():
 	#Action
	if x == 1:	
		c = "Touch "
	elif   x == 2:
		c = "Tickle "
	elif  x == 3 :
		c = "Kiss "
	elif   x == 4:
		c = "Massage "
	#body part
		
	if y == 1:
		d = "Boobs"
	elif  y == 2:
		d = "Ass"
	elif  y  == 3 :
		d = "Lips"
	elif y == 4:
		d = "Vagina"
	print c + d		
 def girlDirty():
	#Action
	if x == 1:
		a = "Lick "
	elif  x == 2:
		a = "Tickle "
	elif x == 3 :
		a = "Kiss "
	elif  x == 4:
		a = "Massage "
		#body part
