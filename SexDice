 gender = raw_input("Are you a guy or a girl?\n>\t")
 state = raw_input("Clean or dirty?\n>\t")
 if gender.lower() == "guy" and state.lower() == "clean":
	guyClean()
 elif gender.lower() == "guy" and state.lower() == "dirty":
 	guyDirty()
 elif gender.lower() == "girl" and state.lower() == "clean":
 	girlClean()
 elif gender.lower() == "girl" and state.lower() == "dirty":
 	girlDirty()
 else:
	print("Write either guy/girl and clean/dirty.")	
